{"ast":null,"code":"export default {\n  name: \"User\",\n  data() {\n    return {\n      tableData: [],\n      dialogFormVisible: false,\n      multipleSelection: [],\n      form: {},\n      username: \"\",\n      email: \"\",\n      address: \"\",\n      total: 0,\n      size: 5,\n      pageNum: 1\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      this.request.get(\"/user/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.size,\n          username: this.username,\n          email: this.email,\n          address: this.address\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n        this.size = res.data.size;\n      });\n\n      // fetch(\"http://localhost:9090/user/page?pageNum=\" + this.pageNum + \"&pageSize=\" + this.size+\"&username=\"+this.username+\"&email=\"+this.email).then(res => res.json()).then(res => {\n      //   this.tableData = res.data.records;\n      //   this.total = res.data.total;\n      //   this.size = res.data.size;\n      // })\n    },\n\n    handleSizeChange(pageSize) {\n      this.size = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    },\n    handleadd() {\n      this.dialogFormVisible = true;\n      this.form = {};\n    },\n    handleedit(row) {\n      this.form = row;\n      this.dialogFormVisible = true;\n    },\n    del(id) {\n      this.request.delete(\"/user/\" + id, this.form).then(res => {\n        if (res.data) {\n          this.$message.success(\"删除成功！\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"删除失败！\");\n        }\n      });\n    },\n    delbatch() {\n      let ids = this.multipleSelection.map(v => v.id); //存入id\n      this.request.post(\"/user/del/batch\", ids).then(res => {\n        if (res.data) {\n          this.$message.success(\"批量删除成功！\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"批量删除失败！\");\n        }\n      });\n    },\n    handleSelectionChange(val) {\n      //多选\n      this.multipleSelection = val;\n    },\n    save() {\n      this.request.post(\"/user\", this.form).then(res => {\n        if (res.data) {\n          this.$message.success(\"保存成功！\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"保存失败！\");\n        }\n      });\n    },\n    reset() {\n      this.username = \"\";\n      this.email = \"\";\n      this.address = \"\";\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","dialogFormVisible","multipleSelection","form","username","email","address","total","size","pageNum","created","load","methods","request","get","params","pageSize","then","res","records","handleSizeChange","handleCurrentChange","handleadd","handleedit","row","del","id","delete","$message","success","error","delbatch","ids","map","v","post","handleSelectionChange","val","save","reset"],"sources":["src/views/User.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin-bottom: 30px\">\r\n      <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n        <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n        <el-breadcrumb-item>用户管理</el-breadcrumb-item>\r\n      </el-breadcrumb>\r\n    </div>\r\n    <div style=\"margin: 10px 0;\">\r\n      <label>姓名: &nbsp; </label>\r\n      <el-input v-model=\"username\" placeholder=\"请输入姓名\" style=\"width: 200px\" suffix-icon=\"el-icon-user\"></el-input>\r\n      <label class=\"ml-5\">邮箱: </label>\r\n      <el-input v-model=\"email\" placeholder=\"请输入邮箱\" style=\"width: 200px\" class=\"ml-10\"\r\n                suffix-icon=\"el-icon-message\"></el-input>\r\n      <label class=\"ml-5\">地址: </label>\r\n      <el-input v-model=\"address\" placeholder=\"请输入地址\" style=\"width: 200px\" class=\"ml-10\"\r\n                suffix-icon=\"el-icon-message\"></el-input>\r\n      <el-button type=\"primary\" @click=\"load\" class=\"ml-10\" icon=\"el-icon-search\">搜索</el-button>\r\n      <el-button type=\"warning\" @click=\"reset\" class=\"ml-10\">重置</el-button>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0;\">\r\n      <el-button type=\"primary\" size=\"mini\" @click=\"handleadd\" icon=\"el-icon-plus\" plain>新增</el-button>\r\n      <el-popconfirm\r\n          confirm-button-text='确定'\r\n          cancel-button-text='我再想想'\r\n          icon=\"el-icon-danger\"\r\n          icon-color=\"red\"\r\n          title=\"您确定删除吗？\"\r\n          @confirm=\"delbatch\"\r\n      >\r\n        <el-button type=\"danger\" class=\"ml-10\" slot=\"reference\" size=\"mini\" icon=\"el-icon-remove-outline\" plain>\r\n          批量删除\r\n        </el-button>\r\n      </el-popconfirm>\r\n      <el-button type=\"primary\" size=\"mini\" class=\"ml-10\" icon=\"el-icon-remove-outline\" plain>批量删除</el-button>\r\n      <el-button type=\"primary\" size=\"mini\" icon=\"el-icon-remove-outline\" plain>批量删除</el-button>\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\" border stripe header-cell-class-name=\"headBg\"\r\n              @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n      <el-table-column prop=\"username\" label=\"用户名\" width=\"140\" align=\"center\"></el-table-column>\r\n      <el-table-column prop=\"nickname\" label=\"昵称\" width=\"120\" align=\"center\"></el-table-column>\r\n      <el-table-column prop=\"phone\" label=\"电话\" width=\"320\" align=\"center\"></el-table-column>\r\n      <el-table-column prop=\"address\" label=\"地址\" width=\"320\" align=\"center\"></el-table-column>\r\n      <el-table-column prop=\"email\" label=\"邮箱\" width=\"320\" align=\"center\"></el-table-column>\r\n      <el-table-column label=\"操作\">\r\n        <template v-slot=\"scope\">\r\n          <el-button type=\"success\" @click=\"handleedit(scope.row)\" plain><i class=\"el-icon-edit\" size=\"mini\"></i>编辑\r\n          </el-button>\r\n          <el-popconfirm\r\n              confirm-button-text='确定'\r\n              cancel-button-text='我再想想'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"您确定删除吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\" class=\"ml-5\" plain><i class=\"el-icon-remove-outline\"\r\n                                                                            size=\"mini\"></i>删除\r\n            </el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[5, 10, 15, 20]\"\r\n          :page-size=\"size\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <el-dialog title=\"用户信息\" :visible.sync=\"dialogFormVisible\" width=\"30%\">\r\n      <el-form label-width=\"70px\" size=\"small\">\r\n        <el-form-item label=\"用户名\">\r\n          <el-input v-model=\"form.username\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"昵称\">\r\n          <el-input v-model=\"form.nickname\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"电话\">\r\n          <el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"邮箱\">\r\n          <el-input v-model=\"form.email\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"地址\">\r\n          <el-input v-model=\"form.address\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"User\",\r\n  data(){\r\n    return{\r\n      tableData: [],\r\n      dialogFormVisible: false,\r\n      multipleSelection: [],\r\n      form: {},\r\n      username: \"\",\r\n      email: \"\",\r\n      address: \"\",\r\n      total: 0,\r\n      size: 5,\r\n      pageNum: 1\r\n    }\r\n  },\r\n  created() {\r\n    this.load();\r\n  },\r\n  methods:{\r\n    load() {\r\n      this.request.get(\"/user/page\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.size,\r\n          username: this.username,\r\n          email: this.email,\r\n          address: this.address\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data.records;\r\n        this.total = res.data.total;\r\n        this.size = res.data.size;\r\n      })\r\n\r\n      // fetch(\"http://localhost:9090/user/page?pageNum=\" + this.pageNum + \"&pageSize=\" + this.size+\"&username=\"+this.username+\"&email=\"+this.email).then(res => res.json()).then(res => {\r\n      //   this.tableData = res.data.records;\r\n      //   this.total = res.data.total;\r\n      //   this.size = res.data.size;\r\n      // })\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      this.size = pageSize;\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.pageNum = pageNum;\r\n      this.load()\r\n    },\r\n    handleadd() {\r\n      this.dialogFormVisible = true;\r\n      this.form = {}\r\n    },\r\n    handleedit(row) {\r\n      this.form = row\r\n      this.dialogFormVisible = true;\r\n    },\r\n    del(id) {\r\n      this.request.delete(\"/user/\" + id, this.form).then(res => {\r\n        if (res.data) {\r\n          this.$message.success(\"删除成功！\");\r\n          this.dialogFormVisible = false;\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"删除失败！\");\r\n        }\r\n      })\r\n    },\r\n    delbatch() {\r\n      let ids = this.multipleSelection.map(v => v.id);//存入id\r\n      this.request.post(\"/user/del/batch\", ids).then(res => {\r\n        if (res.data) {\r\n          this.$message.success(\"批量删除成功！\");\r\n          this.dialogFormVisible = false;\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"批量删除失败！\");\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val) {//多选\r\n      this.multipleSelection = val;\r\n    },\r\n    save() {\r\n      this.request.post(\"/user\", this.form).then(res => {\r\n        if (res.data) {\r\n          this.$message.success(\"保存成功！\");\r\n          this.dialogFormVisible = false;\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"保存失败！\");\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.username = \"\";\r\n      this.email = \"\";\r\n      this.address = \"\"\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.headBg {\r\n  background: #eee !important;\r\n}\r\n</style>\r\n"],"mappings":"AAyGA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,iBAAA;MACAC,iBAAA;MACAC,IAAA;MACAC,QAAA;MACAC,KAAA;MACAC,OAAA;MACAC,KAAA;MACAC,IAAA;MACAC,OAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACA,KAAAE,OAAA,CAAAC,GAAA;QACAC,MAAA;UACAN,OAAA,OAAAA,OAAA;UACAO,QAAA,OAAAR,IAAA;UACAJ,QAAA,OAAAA,QAAA;UACAC,KAAA,OAAAA,KAAA;UACAC,OAAA,OAAAA;QACA;MACA,GAAAW,IAAA,CAAAC,GAAA;QACA,KAAAlB,SAAA,GAAAkB,GAAA,CAAAnB,IAAA,CAAAoB,OAAA;QACA,KAAAZ,KAAA,GAAAW,GAAA,CAAAnB,IAAA,CAAAQ,KAAA;QACA,KAAAC,IAAA,GAAAU,GAAA,CAAAnB,IAAA,CAAAS,IAAA;MACA;;MAEA;MACA;MACA;MACA;MACA;IACA;;IACAY,iBAAAJ,QAAA;MACA,KAAAR,IAAA,GAAAQ,QAAA;MACA,KAAAL,IAAA;IACA;IACAU,oBAAAZ,OAAA;MACA,KAAAA,OAAA,GAAAA,OAAA;MACA,KAAA
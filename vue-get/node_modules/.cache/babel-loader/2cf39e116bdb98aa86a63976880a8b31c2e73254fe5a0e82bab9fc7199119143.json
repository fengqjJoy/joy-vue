{"ast":null,"code":"export default {\n  name: \"home\",\n  data() {\n    return {\n      tableData: [],\n      collapseBtnclass: 'el-icon-s-fold',\n      isCollapse: false,\n      sideWidth: 200,\n      logoTextShow: true,\n      dialogFormVisible: false,\n      multipleSelection: [],\n      form: {},\n      username: \"\",\n      email: \"\",\n      address: \"\",\n      total: 0,\n      size: 5,\n      pageNum: 1\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    collapse() {\n      this.isCollapse = !this.isCollapse;\n      if (this.isCollapse) {\n        this.sideWidth = 64;\n        this.collapseBtnclass = 'el-icon-s-unfold';\n        this.logoTextShow = false;\n      } else {\n        this.sideWidth = 200;\n        this.collapseBtnclass = 'el-icon-s-fold';\n        this.logoTextShow = true;\n      }\n    },\n    load() {\n      this.request.get(\"/user/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.size,\n          username: this.username,\n          email: this.email,\n          address: this.address\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n        this.size = res.data.size;\n      });\n\n      // fetch(\"http://localhost:9090/user/page?pageNum=\" + this.pageNum + \"&pageSize=\" + this.size+\"&username=\"+this.username+\"&email=\"+this.email).then(res => res.json()).then(res => {\n      //   this.tableData = res.data.records;\n      //   this.total = res.data.total;\n      //   this.size = res.data.size;\n      // })\n    },\n\n    handleSizeChange(pageSize) {\n      this.size = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    },\n    handleadd() {\n      this.dialogFormVisible = true;\n      this.form = {};\n    },\n    handleedit(row) {\n      this.form = row;\n      this.dialogFormVisible = true;\n    },\n    del(id) {\n      this.request.delete(\"/user/\" + id, this.form).then(res => {\n        if (res.data) {\n          this.$message.success(\"删除成功！\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"删除失败！\");\n        }\n      });\n    },\n    delbatch() {\n      let ids = this.multipleSelection.map(v => v.id); //存入id\n      this.request.post(\"/user/del/batch\", ids).then(res => {\n        if (res.data) {\n          this.$message.success(\"批量删除成功！\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"批量删除失败！\");\n        }\n      });\n    },\n    handleSelectionChange(val) {\n      //多选\n      this.multipleSelection = val;\n    },\n    save() {\n      this.request.post(\"/user\", this.form).then(res => {\n        if (res.data) {\n          this.$message.success(\"保存成功！\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"保存失败！\");\n        }\n      });\n    },\n    reset() {\n      this.username = \"\";\n      this.email = \"\";\n      this.address = \"\";\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","collapseBtnclass","isCollapse","sideWidth","logoTextShow","dialogFormVisible","multipleSelection","form","username","email","address","total","size","pageNum","created","load","methods","collapse","request","get","params","pageSize","then","res","records","handleSizeChange","handleCurrentChange","handleadd","handleedit","row","del","id","delete","$message","success","error","delbatch","ids","map","v","post","handleSelectionChange","val","save","reset"],"sources":["src/views/Home.vue"],"sourcesContent":["<template>\n  <el-container style=\"height: 100vh;\">\n    <el-aside :width=\"sideWidth + 'px'\"\n              style=\"background-color: rgb(238, 241, 246); height: 100%;box-shadow: 2px 0 6px rgba(0,21,41,.35)\">\n      <el-menu :default-openeds=\"['1', '3']\" style=\"min-height: 100%;overflow-x: hidden\"\n               background-color=\"rgb(48,65,86)\"\n               text-color=\"#fff\" active-text-color=\"#ffd04b\" :coTlapse-transition=\"false\" :collapse=\"isCollapse\">\n        <div style=\"height: 60px; line-height: 60px; text-align: center\">\n          <img src=\"../assets/logo.png\" alt=\"\" style=\"width: 20px;position: relative;top:5px;margin-right: 5px\">\n          <b style=\"color: white\" v-show=\"logoTextShow\">后台管理系统</b>\n        </div>\n        <el-submenu index=\"1\">\n          <template slot=\"title\"><i class=\"el-icon-message\"></i>\n            <span slot=\"title\">导航一</span>\n          </template>\n          <el-menu-item-group title=\"分组2\">\n            <el-menu-item index=\"1-3\">选项3</el-menu-item>\n          </el-menu-item-group>\n          <el-submenu index=\"1-4\">\n            <template slot=\"title\">选项4</template>\n            <el-menu-item index=\"1-4-1\">选项4-1</el-menu-item>\n          </el-submenu>\n        </el-submenu>\n        <el-submenu index=\"2\">\n          <template slot=\"title\"><i class=\"el-icon-menu\"></i><span slot=\"title\">导航二</span></template>\n          <el-menu-item-group title=\"分组2\">\n            <el-menu-item index=\"2-3\">选项3</el-menu-item>\n          </el-menu-item-group>\n          <el-submenu index=\"2-4\">\n            <template slot=\"title\">选项4</template>\n            <el-menu-item index=\"2-4-1\">选项4-1</el-menu-item>\n          </el-submenu>\n        </el-submenu>\n        <el-submenu index=\"3\">\n          <template slot=\"title\"><i class=\"el-icon-setting\"></i><span slot=\"title\">导航三</span></template>\n          <el-menu-item-group title=\"分组2\">\n            <el-menu-item index=\"3-3\">选项3</el-menu-item>\n          </el-menu-item-group>\n          <el-submenu index=\"3-4\">\n            <template slot=\"title\">选项4</template>\n            <el-menu-item index=\"3-4-1\">选项4-1</el-menu-item>\n          </el-submenu>\n        </el-submenu>\n      </el-menu>\n    </el-aside>\n\n    <el-container>\n      <el-header style=\"font-size: 12px; line-height: 60px; border-bottom: 1px solid ; display: flex\">\n        <div style=\"flex: 1; font-size: 20px\">\n          <span :class=\"collapseBtnclass\" style=\"cursor: pointer\" @click=\"collapse\"></span>\n        </div>\n        <el-dropdown style=\"width: 100px;cursor: pointer;\">\n          <span>王小虎</span><i class=\"el-icon-arrow-down\" style=\"margin-left: 5px\"></i>\n          <el-dropdown-menu slot=\"dropdown\">\n            <el-dropdown-item>个人信息</el-dropdown-item>\n            <el-dropdown-item>修改密码</el-dropdown-item>\n            <el-dropdown-item>退出</el-dropdown-item>\n          </el-dropdown-menu>\n        </el-dropdown>\n      </el-header>\n      <el-main>\n        <div style=\"margin-bottom: 30px\">\n          <el-breadcrumb separator-class=\"el-icon-arrow-right\">\n            <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\n            <el-breadcrumb-item>用户管理</el-breadcrumb-item>\n          </el-breadcrumb>\n        </div>\n        <div style=\"margin: 10px 0;\">\n          <label>姓名: &nbsp; </label>\n          <el-input v-model=\"username\" placeholder=\"请输入姓名\" style=\"width: 200px\" suffix-icon=\"el-icon-user\"></el-input>\n          <label class=\"ml-5\">邮箱: </label>\n          <el-input v-model=\"email\" placeholder=\"请输入邮箱\" style=\"width: 200px\" class=\"ml-10\"\n                    suffix-icon=\"el-icon-message\"></el-input>\n          <label class=\"ml-5\">地址: </label>\n          <el-input v-model=\"address\" placeholder=\"请输入地址\" style=\"width: 200px\" class=\"ml-10\"\n                    suffix-icon=\"el-icon-message\"></el-input>\n          <el-button type=\"primary\" @click=\"load\" class=\"ml-10\" icon=\"el-icon-search\">搜索</el-button>\n          <el-button type=\"warning\" @click=\"reset\" class=\"ml-10\">重置</el-button>\n        </div>\n\n        <div style=\"margin: 10px 0;\">\n          <el-button type=\"primary\" size=\"mini\" @click=\"handleadd\" icon=\"el-icon-plus\" plain>新增</el-button>\n          <el-popconfirm\n              confirm-button-text='确定'\n              cancel-button-text='我再想想'\n              icon=\"el-icon-info\"\n              icon-color=\"red\"\n              title=\"您确定删除吗？\"\n              @confirm=\"delbatch\"\n          >\n          <el-button type=\"danger\"  class=\"ml-5\" slot=\"reference\" size=\"mini\" icon=\"el-icon-remove-outline\" plain>批量删除</el-button>\n          </el-popconfirm>\n          <el-button type=\"primary\" size=\"mini\" class=\"ml-5\" icon=\"el-icon-remove-outline\" plain>批量删除</el-button>\n          <el-button type=\"primary\" size=\"mini\" icon=\"el-icon-remove-outline\" plain>批量删除</el-button>\n        </div>\n\n        <el-table :data=\"tableData\" border stripe header-cell-class-name=\"headBg\"\n                  @selection-change=\"handleSelectionChange\">\n          <el-table-column type=\"selection\" width=\"55\"></el-table-column>\n          <el-table-column prop=\"username\" label=\"用户名\" width=\"140\" align=\"center\"></el-table-column>\n          <el-table-column prop=\"nickname\" label=\"昵称\" width=\"120\" align=\"center\"></el-table-column>\n          <el-table-column prop=\"phone\" label=\"电话\" width=\"320\" align=\"center\"></el-table-column>\n          <el-table-column prop=\"address\" label=\"地址\" width=\"320\" align=\"center\"></el-table-column>\n          <el-table-column prop=\"email\" label=\"邮箱\" width=\"320\" align=\"center\"></el-table-column>\n          <el-table-column label=\"操作\">\n            <template v-slot=\"scope\">\n              <el-button type=\"success\" @click=\"handleedit(scope.row)\" plain><i class=\"el-icon-edit\" size=\"mini\"></i>编辑\n              </el-button>\n              <el-popconfirm\n                  confirm-button-text='确定'\n                  cancel-button-text='我再想想'\n                  icon=\"el-icon-info\"\n                  icon-color=\"red\"\n                  title=\"您确定删除吗？\"\n                  @confirm=\"del(scope.row.id)\"\n              >\n                <el-button type=\"danger\" slot=\"reference\" class=\"ml-5\" plain><i class=\"el-icon-remove-outline\"\n                                                                                size=\"mini\"></i>删除\n                </el-button>\n              </el-popconfirm>\n            </template>\n          </el-table-column>\n        </el-table>\n        <div style=\"padding: 10px 0\">\n          <el-pagination\n              @size-change=\"handleSizeChange\"\n              @current-change=\"handleCurrentChange\"\n              :current-page=\"pageNum\"\n              :page-sizes=\"[5, 10, 15, 20]\"\n              :page-size=\"size\"\n              layout=\"total, sizes, prev, pager, next, jumper\"\n              :total=\"total\">\n          </el-pagination>\n        </div>\n\n        <el-dialog title=\"用户信息\" :visible.sync=\"dialogFormVisible\" width=\"30%\">\n          <el-form label-width=\"70px\" size=\"small\">\n            <el-form-item label=\"用户名\">\n              <el-input v-model=\"form.username\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n            <el-form-item label=\"昵称\">\n              <el-input v-model=\"form.nickname\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n            <el-form-item label=\"电话\">\n              <el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n            <el-form-item label=\"邮箱\">\n              <el-input v-model=\"form.email\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n            <el-form-item label=\"地址\">\n              <el-input v-model=\"form.address\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n          </el-form>\n          <div slot=\"footer\" class=\"dialog-footer\">\n            <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\n            <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n          </div>\n        </el-dialog>\n\n      </el-main>\n\n    </el-container>\n  </el-container>\n</template>\n\n<script>\n\n\nexport default {\n  name: \"home\",\n  data() {\n    return {\n      tableData: [],\n      collapseBtnclass: 'el-icon-s-fold',\n      isCollapse: false,\n      sideWidth: 200,\n      logoTextShow: true,\n      dialogFormVisible: false,\n      multipleSelection: [],\n      form: {},\n      username: \"\",\n      email: \"\",\n      address: \"\",\n      total: 0,\n      size: 5,\n      pageNum: 1\n    }\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    collapse() {\n      this.isCollapse = !this.isCollapse\n      if (this.isCollapse) {\n        this.sideWidth = 64\n        this.collapseBtnclass = 'el-icon-s-unfold'\n        this.logoTextShow = false\n      } else {\n        this.sideWidth = 200\n        this.collapseBtnclass = 'el-icon-s-fold'\n        this.logoTextShow = true\n      }\n    },\n    load() {\n      this.request.get(\"/user/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.size,\n          username: this.username,\n          email: this.email,\n          address: this.address\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n        this.size = res.data.size;\n      })\n\n      // fetch(\"http://localhost:9090/user/page?pageNum=\" + this.pageNum + \"&pageSize=\" + this.size+\"&username=\"+this.username+\"&email=\"+this.email).then(res => res.json()).then(res => {\n      //   this.tableData = res.data.records;\n      //   this.total = res.data.total;\n      //   this.size = res.data.size;\n      // })\n    },\n    handleSizeChange(pageSize) {\n      this.size = pageSize;\n      this.load()\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load()\n    },\n    handleadd() {\n      this.dialogFormVisible = true;\n      this.form = {}\n    },\n    handleedit(row) {\n      this.form = row\n      this.dialogFormVisible = true;\n    },\n    del(id) {\n      this.request.delete(\"/user/\" + id, this.form).then(res => {\n        if (res.data) {\n          this.$message.success(\"删除成功！\");\n          this.dialogFormVisible = false;\n          this.load()\n        } else {\n          this.$message.error(\"删除失败！\");\n        }\n      })\n    },\n    delbatch(){\n      let ids=this.multipleSelection.map(v=>v.id);//存入id\n      this.request.post(\"/user/del/batch\", ids).then(res => {\n        if (res.data) {\n          this.$message.success(\"批量删除成功！\");\n          this.dialogFormVisible = false;\n          this.load()\n        } else {\n          this.$message.error(\"批量删除失败！\");\n        }\n      })\n    },\n    handleSelectionChange(val) {//多选\n      this.multipleSelection = val;\n    },\n    save() {\n      this.request.post(\"/user\", this.form).then(res => {\n        if (res.data) {\n          this.$message.success(\"保存成功！\");\n          this.dialogFormVisible = false;\n          this.load()\n        } else {\n          this.$message.error(\"保存失败！\");\n        }\n      })\n    },\n    reset() {\n      this.username = \"\";\n      this.email = \"\";\n      this.address = \"\"\n    }\n  }\n}\n</script>\n<style>\n.el-header {\n  color: #333;\n  line-height: 60px;\n}\n\n.el-aside {\n  color: #333;\n}\n\n.headBg {\n  background: #eee !important;\n}\n</style>\n"],"mappings":"AAwKA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,gBAAA;MACAC,UAAA;MACAC,SAAA;MACAC,YAAA;MACAC,iBAAA;MACAC,iBAAA;MACAC,IAAA;MACAC,QAAA;MACAC,KAAA;MACAC,OAAA;MACAC,KAAA;MACAC,IAAA;MACAC,OAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAC,SAAA;MACA,KAAAf,UAAA,SAAAA,UAAA;MACA,SAAAA,UAAA;QACA,KAAAC,SAAA;QACA,KAAAF,gBAAA;QACA,KAAAG,YAAA;MACA;QACA,KAAAD,SAAA;QACA,KAAAF,gBAAA;QACA,KAAAG,YAAA;MACA;IACA;IACAW,KAAA;MACA,KAAAG,OAAA,CAAAC,GAAA;QACAC,MAAA;UACAP,OAAA,OAAAA,OAAA;UACAQ,QAAA,OAAAT,IAAA;UACAJ,QAAA,OAAAA,QAAA;UACAC,KAAA,OAAAA,KAAA;UACAC,OAAA,OAAAA;QACA;MACA,GAAAY,IAAA,CAA